    #Trademark String
    nameid 7 "Sector is a trademark of Gunnar Vilhjalmsson; Montserrat is a trademark of Julieta Ulanovsky";
    nameid 7 1 "Sector is a trademark of Gunnar Vilhjalmsson; Montserrat is a trademark of Julieta Ulanovsky";
    
    #Manufacturer Name
    nameid 8 "UT";
    nameid 8 1 "UT";
    
    #Designer
    nameid 9 "Gunnar Vilhjalmsson";
    nameid 9 1 "Gunnar Vilhjalmsson";
    
    #URL Vendor
    #nameid 11 "";
    #nameid 11 1 "";
    
    #URL Designer
    nameid 12 "www.gunnarvilhjalmsson.net";
    nameid 12 1 "www.gunnarvilhjalmsson.net";
    
    #License Description
    nameid 13 "This Font Software is licensed under the SIL Open Font License, Version 1.1.";
    nameid 13 1 "This Font Software is licensed under the SIL Open Font License, Version 1.1.";
    
    #License URL
    nameid 14 "http://scripts.sil.org/OFL";
    nameid 14 1 "http://scripts.sil.org/OFL";
