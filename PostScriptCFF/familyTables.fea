table GDEF {
	# 1. <@SIMPLE> : Spacing Base Glyph.
	# 2. <@LIGATURES> : Spacing Base Glyph.
	# 3. <@MARK> : Non-Spacing Glyph.
	# 4. <@COMPONENT> : Part of a Base Glyph.
#	GlyphClassDef @SIMPLE, @LIGATURES, @MARK, @COMPONENT;
    GlyphClassDef , @LIGATURES, @MARK, @COMPONENT;
